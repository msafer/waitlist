{
  "name": "farcaster-app",
  "version": "1.0.0",
  "description": "A Farcaster application built with Neynar SDK",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "farcaster",
    "neynar",
    "typescript",
    "backend"
  ],
  "author": "Marcus Safer",
  "license": "MIT",
  "dependencies": {
    "@neynar/nodejs-sdk": "^3.34.0",
    "@prisma/client": "^6.14.0",
    "@sentry/node": "^7.100.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "graphql-request": "^7.2.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1",
    "prisma": "^6.14.0",
    "rate-limit-redis": "^4.2.0",
    "siwe": "^3.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/express-session": "^1.18.2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
